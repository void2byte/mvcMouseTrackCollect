    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Debug Qt Application",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/debug/mvcMouseTrackCollect.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "PATH",
                    "value": "${env:PATH};C:/Qt/6.7.2/mingw_64/bin;C:/Qt/Tools/mingw1310_64/bin"
                }
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "C:/Qt/Tools/mingw1310_64/bin/gdb.exe",
            "setupCommands": [
                {
                    "description": "Включить автоматическое форматирование для gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build"
        }
    ]
}